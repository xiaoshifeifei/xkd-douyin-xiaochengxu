page {
  font-size: 24rpx;
}

.jszc{
  height: 80rpx;
  width: 100%;
  text-align: center;
  line-height: 80rpx;
  overflow: hidden;
  color: #ccc;
}

.kongbai{
  width: 100%;
  height: 100rpx;
}

.footer {
  height: 100rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  background: #fff;
  border-top: 1rpx solid #e2e2e2;
  z-index: 10000;
}

.footer .ft_item {
  width: 25%;
  height: 100%;
  text-align: center;
  float: left;
  color: #666;
}

.footer .ft_item .image {
  height: 45rpx;
  width: 45rpx;
  margin: 0 auto;
  display: block;
  margin-top: 10rpx;
  font-size: 45rpx;
}

.footer .ft_item image {
  height: 45rpx;
  width: 45rpx;
  margin: 0 auto;
  display: block;
  margin-top: 10rpx;
}

.footer  .ft_item text {
  text-align: center;
  font-size: 26rpx;
  display: block;
  margin: 0 auto;
  margin-top: 10rpx;
}

.more,.nomore{
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    font-size: 26rpx;
    background: #fff;
    width: 100%;
    margin: 0 auto;
    margin-top: 10rpx;
}

.nomore{
  color: #ccc; 
}

@font-face {
  font-family: 'iconfont';
  src: url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
         url('iconfont.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-zuopin:before {
  content: "\e61b";
}

.icon-icon-yxj-empty-my:before {
  content: "\e651";
}

.icon-qiajuan:before {
  content: "\e653";
}

.icon-APPtubiao-:before {
  content: "\e602";
}

/**
**优惠券css
***/

.card_bg_01{background: #99499C;}
.card_bg_02{background: #FF5500;}
.card_bg_03{background: #01B01E;}
.card_bg_04{background: #fff;}

.cfont_color_01{color: #99499C;}
.cfont_color_02{color: #FF5500;}
.cfont_color_03{color: #01B01E;}

.ditem {
  background-color: #fff;
  margin: 0 4%;
  margin-top: 20rpx;
  box-shadow: 2px 2px 1px #eaeaea;
  border-radius: 10rpx;
}

.ditem .dtview {
  height: 160rpx;
  padding: 0 4%;
    display: flex;
  justify-content: space-between;
  align-items: center;
}

.ditem .dtview .dface {
  width: 22%;
  height: 160rpx;
}

.ditem .dtview .dface image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 60rpx;
   border: 1px solid #f0f0f0;
  margin: 30rpx 18rpx;
}

.ditem .dtview .dcontent {
  height: 160rpx;
  margin-left: 30rpx;
  width: 78%;
}

.ditem .dtview .dcontent .dinfo {
  height: auto;
  font-size: 26rpx;
  margin-top: 10rpx;
  color: #8a8a8a;
}
.ditem .dtview .dcontent .dtitle {
  margin-top: 30rpx;
}
.ditem .dtview .dreceive {
  width: 26%;
  height: 160rpx;
  float: right;
}

.ditem .dtview .dreceive .btn {
  width: auto;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  padding: 0 20rpx;
  margin: 40rpx 10rpx;
  background: #fff;
  border-radius: 5rpx;
  font-size: 28rpx;
}

.ditem .dbview {
  padding: 0 30rpx;
  height: 60rpx;
  line-height: 60rpx;
}

.ditem .dbview .ldb {

  height: auto;
  float: left;
  font-size: 24rpx;
  color: #8a8a8a;
}

.ditem .dbview .rdb {

  height: auto;
  float: right;
  font-size: 24rpx;
  color: #8a8a8a;
}

.name{
  padding-left: 15rpx;
  padding-top: 15rpx;
  font-size: 30rpx;
  font-weight: bold;
}
.footerButton{
  padding: 20rpx 15rpx;
}
.float-right{
  float: right;
  margin-left: 10rpx;
}
.make-button{
  margin-top: 30rpx;
  margin-left: 30rpx;  
}

/* 悬浮框样式 */
.round-click{
    height: 120rpx;
    width: 120rpx;
    /* background-color: #d92a2a; */
    
	background-color: #146eeb;
  opacity: 0.8;
    border-radius: 100%;
    position: fixed;
    bottom: 150rpx;
    right: 20rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 999;
}
.round-click navigator{
    font-size: 32rpx;
    max-width: 80rpx;
    color: #fff;
    text-align: center;
}
.round-click view{
    font-size: 32rpx;
    max-width: 80rpx;
    color: #fff;
    text-align: center;
}
